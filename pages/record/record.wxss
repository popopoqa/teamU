page {
  background:#F9F9F9;
}
.container {
  position: relative;
}
.tabNav {
  z-index: 4; 
  position: fixed; 
  top:0;
  left:0; 
  width:100%; 
  height:80rpx; 
  line-height: 80rpx; 
  background: #fff; 
  display: flex; 
  padding:0 30rpx; 
  border-bottom:1px solid #f5f5f5; 
  box-sizing: border-box; 
}
.tabNav> view {
  width:49.9%;
  font-size:28rpx;
  text-align: center; 
  color:#616161;
}
.tabNav> view text { 
  padding:0 15rpx; 
  height:75rpx; 
  display:inline-block;
}
.tabNav .cur text { 
  border-bottom:5rpx solid #000000; 
  color:#000000;
}
.box {
  margin-top:100rpx;
  background: #FFFFFF;
}
.box .record {
  color:#818181;
  margin:0 30rpx;
  padding:10rpx 0;
  border-bottom:1rpx solid #F7F7F7;
}
.box .record .msg {
  font-size:26rpx;
}
.box .record .msg .p2 {
  float: right;
  font-size:24rpx;
}
.box .record .message {
  font-size:24rpx;
  margin-top:20rpx;
}
.box .record .message p {
  margin-left:30rpx;
}
.box .record .message p:nth-child(1) {
  margin-left:0;
}
.container111 {
  margin-top:80rpx;
}
.body {
  margin-top:500rpx;
  text-align: center;
  color:#717171;
  font-size:26rpx;
}
