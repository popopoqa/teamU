/* pages/search/search.wxss */
.search {
  display:flex;
  align-items: center;
  margin:0 30rpx;
  background:#F4F4F4;
  font-size:28rpx;
  color:#989898;
  border-radius: 12rpx;
  padding:10rpx 0;
}
.search i {
 color:#2C2C2C;
 margin:0 10rpx 0 20rpx;
 font-size:26rpx;
}
.box{
  border-top:1rpx solid #DCDCDC;
  margin-top:40rpx;
}
.box .park {
  border-bottom:1rpx solid #E5E5E5;
  padding:15rpx 30rpx;
}
.box .park .basketball {
  color:#040404;
  font-size:32rpx;
}
.box .park .mall {
  font-size:26rpx;
  background: #FFC492;
  color:#CC4613;
  padding:5rpx;
  border-radius: 10rpx;
  margin-left:10rpx;
}
