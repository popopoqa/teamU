page {
  background:#F8F8F8;
  color:#9A9A9A;
}
.coupon {
  background:#FFFFFF;
  margin:30rpx 30rpx 0 30rpx;
  padding:30rpx;
  border-radius:12rpx;
  margin-bottom:30rpx;
}
.coupon>.money {
  float:left;
  border-right: 1rpx solid #9A9A9A;
  padding-right:30rpx;
  margin-right:10rpx;
  padding:10rpx 30rpx 10rpx 0;
  text-align: center;
}
.coupon>.money>view {
  font-size:32rpx;
}
.coupon>.money>view:nth-child(2) {
  font-size:26rpx;
  margin-top:20rpx;
}
.give {
  padding-left:10rpx;
}
.coupon .give view:nth-child(1) {
  font-size:32rpx;
}
.coupon .give view {
  margin-top:10rpx;
  font-size:26rpx;
}
.temporary {
  position:absolute;
  color:#A8A8A8;
  font-size:30rpx;
  width:100%;
  height:100%;
  display: flex;
  align-items:center;
  justify-content: center;
} 
