page {
  background:#F9F9F7;
}
.container {
  position: relative;
  padding-bottom:200rpx;
}
.tabNav {
  z-index: 4; 
  position: fixed; 
  top:0;
  left:0; 
  width:100%; 
  height:80rpx; 
  line-height: 80rpx; 
  background: #fff; 
  display: flex; 
  padding:0 30rpx; 
  border-bottom:1px solid #f5f5f5; 
  box-sizing: border-box; 
}
.tabNav> view {
  width:49.9%;
  font-size:28rpx;
  text-align: center; 
  color:#616161;
}
.tabNav> view text { 
  padding:0 15rpx; 
  height:75rpx; 
  display:inline-block;
}
.tabNav .cur text { 
  border-bottom:5rpx solid #000000; 
  color:#000000;
}
.orderInfo {
  margin-top:100rpx;
  margin-bottom:200rpx;
}
.orderInfo .orderInfo-item .tickets {
  font-size:28rpx;
  color:#777777;
  text-align:center;
  position:fixed;
  top:40%;
  width:100%;
}
.orderInfo .orderInfo-item .sceneInformation {
  margin-top:30rpx;
}
.orderInfo .orderInfo-item .sceneInformation .msg {
  background:#52D0B8;
  font-size:32rpx;
  color:#EDFFFF;
  display:flex;
  height:130rpx;
  padding:30rpx 30rpx 0 30rpx;
}
.orderInfo .orderInfo-item .sceneInformation .msg .test {
  display:flex;
  flex:1;
}
.orderInfo .orderInfo-item .sceneInformation .msg .test image {
  width:100rpx;
  height:60rpx;
  border:3rpx solid #FFFFFF;
  margin-right: 30rpx;
}
.orderInfo .orderInfo-item .sceneInformation .openTime {
  width:90%;
  margin:0 auto;
  margin-top:-40rpx;
  z-index:300;
}
.orderInfo .orderInfo-item .sceneInformation .openTime .workingDay {
  background:#FFFFFF;
  display:flex;
  padding:30rpx;
  align-items: center;
  border-radius: 20rpx;
  border-bottom:1rpx solid #F9F9F7;
}
.orderInfo .orderInfo-item .sceneInformation .openTime .workingDay .box {
  color:#6E6E6E;
  flex:1;
}
.orderInfo .orderInfo-item .sceneInformation .openTime .workingDay .box .workday {
  font-size:30rpx;
}
.orderInfo .orderInfo-item .sceneInformation .openTime .workingDay .box .time {
  font-size:26rpx;
  margin-top:5rpx;
}
.orderInfo .orderInfo-item .sceneInformation .openTime .workingDay .admissionTicket {
  background: #7EDDFB;
  color:#FFFFFF;
  font-size:30rpx;
  padding:0 30rpx;
  height:60rpx;
  line-height:60rpx;
  border-radius:10rpx;
}
.orderInfo .orderInfo-item .sceneInformation .openTime .workingDay  .member {
  background:#FDDE28;
}
.orderInfo .orderInfo-item .sceneInformation .openTime .workingDay  .package {
  background:#FF815B;
}
.suspensionWindow { 
  position: fixed;
  bottom:130rpx;
  right:0rpx;
}
.suspensionWindow .L_img1 {
  background: #F2F2F2;
  margin-top:30rpx;
  border-bottom-left-radius:60rpx;
  border-top-left-radius:60rpx;
  box-shadow: 0 0 2rpx 2rpx #E5E3E4;
  padding-left:10rpx;
}
.suspensionWindow  image{
  width:80rpx;
  height:80rpx;
  display:block;
}
/* 登录部分 */
.L_container .bcg {
  display:block;
  text-align: center;
  margin-top:200rpx;
}
.L_container .bcg image{
  width:120rpx;
  height:120rpx;
  border-radius: 50%;
  filter: brightness(95%);
}
.L_container .L_lookup {
  text-align:center;
  margin-top:60rpx;
  font-size:26rpx;
}
.L_container .L_login {
  width:120rpx;
  font-size:26rpx;
  margin-top:30rpx;
  background:#21C68A;
  color:#FFFFFF;
}
.suspensionWindow {
  position: fixed;
  bottom:310rpx;
  right:0;
  z-index:0;
  border:1rpx solid #E0DFE0;
  padding-left:5rpx;
  border-top-left-radius:60rpx;
  border-bottom-left-radius:60rpx;
  background: #F3F3F3;
}
.suspensionWindow  .pic1 {
  width:100rpx;
  height:100rpx;
  border-radius: 50%;
  display:block;
}
.suspensionWindow image {
  border-radius: 50%;
}
